@model DAL.Entities.Institutional
@{
    ViewBag.Title = InstitutionalRess.InstitutionalStrings.title;
    Layout = "~/Views/Shared/frontlayout.cshtml";
}
@section breadcrumb{
    <div class="col-md-8 breadcrumb">
        <ul>
            <li><a href="@InstitutionalRess.InstitutionalStrings.breadcrumbmainurl">@InstitutionalRess.InstitutionalStrings.breadcrumbmaintext</a></li>
            <li>@InstitutionalRess.InstitutionalStrings.breadcrumbtext</li>
        </ul>
    </div>
}

<div id="pageHeader">
    <div class="pageHeaderImg">
        <img src="/Content/images/pageHeader.jpg" />
    </div>
</div>
<div id="container" class="container">
    <div class="col-md-12 shadow section noPadding">
        <div class="col-md-12 sectionContent page">
            <h2 class="pageTitle">
                <i class="fa fa-angle-double-right"></i>
                @InstitutionalRess.InstitutionalStrings.header
            </h2>
            @Html.Raw(Model.Content)
        </div>
    </div>
</div>

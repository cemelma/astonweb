<script src="~/js/jquery.cycle.all.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.tab a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
        });

        $(".news-slider").after('<div id="nav">')
		.cycle({
		    fx: 'slideY',
		    speed: 300,
		    timeout: 0,
		    pager: '#nav'
		});

        $(".slider").after('<div id="kav">')
		.cycle({
		    fx: 'fade',
		    speed: 300,
		    timeout: 0,
		    pager: '#kav'
		});
    });
</script>

<!doctype html>
<html>
<head>
<meta charset="utf-8">
<title>@ViewBag.Title</title>
<link rel="stylesheet" type="text/css" href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&subset=latin,latin-ext">
<link rel="stylesheet" type="text/css" href="/Content/Styles/bootstrap.min.css">
<link rel="stylesheet" type="text/css" href="/Content/Styles/style.css">
<link rel="stylesheet" type="text/css" href="/Content/Styles/font-awesome.min.css" />
<link rel="stylesheet" type="text/css" href="/Content/Styles/jquery.bxslider.css" />
<link rel="shortcut icon" href="/favicon.ico">
<!--[if lt IE 9]>
	<script src="~/Content/Scripts/html5shiv.js"></script>
	<![endif]-->
<script src="~/Content/Scripts/jquery-1.8.3.min.js"></script>
<script src="~/Content/Scripts/bootstrap.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('.tab a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
        })
    });
</script>
    <script src="/Content/Scripts/jquery.bxslider.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        $('.slider').bxSlider({
            auto: true,
            controls: true
        });
        $('.newsSlider').bxSlider({
            auto: true,
            slideWidth: 295,
            minSlides: 3,
            maxSlides: 3,
            slideMargin: 15,
            pause: 8000,
            speed: 1500,
            controls: false
        });
        $('.referenceSlider').bxSlider({
            auto: true,
            slideWidth: 295,
            minSlides: 5,
            maxSlides: 5,
            slideMargin: 15,
            pause: 8000,
            speed: 1500,
            controls: false
        });
        $('.tab a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
        })
    });
</script>
</head>

<body>
<div id="header" class="container">
  <div class="col-md-12 noPadding">
    <div class="col-md-3 noPadding"> <a href="/" class="logo"><img src="/Content/Images/logo.png" border="0" /></a> </div>
    <div class="col-md-9 noPadding">
      <div class="col-md-12 col-md-pull-0 noPadding headerTop">
          
          @if (Session["userlogin"] != null)
          { 
              <span style="float: right;position: absolute;right: 441px;top: 4px;"><a href="/guvenlicikis">Güvenli Çıkış</a></span>
          }
          
          @{ Html.RenderAction("_search", "FSearch"); }

            <ul class="nav nav-pills topMenu">
              <li class="first"><a href="index.html">TR</a></li>
              <li><a href="#">EN</a></li>
              <li class="last"><a href="#">العربية</a></li>
            </ul>

      </div>

        @{Html.RenderAction("GetTopMenu", "FHome");}

    </div>
  </div>
</div>
    
<div class="col-md-12 noPadding breadcrumbOut">
    @RenderSection("breadcrumb", false)
</div>

<div id="container" class="container">
     @RenderBody()
</div>

@Html.Partial("Partial/_footernew");
@Html.Raw(BLL.AccountBL.AnalyticManager.GetAnalytic().Code)
</body>
</html>

